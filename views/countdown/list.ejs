<div id="list-head">
  <div id="listTabs">
    <input type="button" id="preset" value="Preset Countdowns">
    <input type="button" id="custom" value="Custom Countdowns">
  </div>
  <div id="listLayout">
    <button id="rows" class="active"><img src="/img/list.svg"></button><!--Make the active display stored in a cookie-->
    <button id="grid"><img src="/img/grid.svg"></button>
  </div>
</div>
<div id="presets-list" class="list-menu list">
  <div id="list-title" class="list-item">
    <div class="name">Name</div>
    <div class="date">Next occurrence</div>
    <div class="calendar">Calendar</div>
  </div>
  <% preset = JSON.parse(preset) %>
  <% for (let i of preset) { %>
    <a class="list-item" href="/countdown/<%= i.id %>">
      <img src="<%= i.icon %>">
      <div class="name"><%= i.name %></div>
      <div class="date"><%= i.timing %></div>
      <div class="calendar"><%= i.calendar %></div>
    </a>
  <% } %>
</div>